{% extends "::frontend.html.twig" %}
{% block title %}{{ title }}{% endblock%}
{% block description %}{{ description }}{% endblock%}
{% block content %}
    <h1>{{ title }}</h1>
    <section>
    {% for post in pagination %}
            {% include 'BlogBundle:Frontend/Post:item.html.twig' with {'post': post} %}
    {% else %}
        <p>No hay elementos</p>
    {% endfor %}
    </section>
{% endblock %}

